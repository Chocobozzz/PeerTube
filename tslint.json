{
  "extends": "tslint-config-standard",
  "rules": {
    "await-promise": [true, "Bluebird"],
    "no-inferrable-types": true,
    "eofline": true,
    "indent": [true, "spaces"],
    "ter-indent": [true, 2],
    "max-line-length": [true, 140],
    "no-unused-variable": false, // Memory issues
    "no-floating-promises": false
  }
}
